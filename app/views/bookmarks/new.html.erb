<div class="container">
  <div class="row justify-content-center">
    <div class="col-12 col-md-8">

      <%= simple_form_for [@list, @bookmark] do |f| %>
        <%= f.input :comment %>
        <%= f.association :movie %>
        <%= f.submit "Add Bookmark", class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
